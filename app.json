{
      "name": "HydraUserBot V-7",
      "description": "An Anime themed Telegram userbot.",
      "logo": "https://telegra.ph/file/af2a44b84e649a0a60679.jpg",
      "keywords": [
         "telegram",
         "anime",
         "advanced",
         "userbot",
         "HydraRobot"
      ],   
   "repository": "https://github.com/Otazuki004/HydraUBV-7",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "BOT_TOKEN": {
         "description": "Your bot token. Get one from @BotFather duh",
         "required": false,
         "value": ""
      },
      "API_ID": {
         "description": "Get API_ID from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },    
      "SESSION": {
         "description": "Your Pyrogram String Session.",
         "required": true,
         "value": ""
     }
         }
      }
